{{#each locations}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
    </h2>
    <a href="/location/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-folder-open"></i>
      </span>
    </a>

    <a href="/dashboard/delete/{{_id}}" class="button">
      <span class="icon is-small">
        <i class="fas fa-trash-alt"></i>
      </span>
    </a>
  </div>
{{/each}}